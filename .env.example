# API Keys Example File
# Copy this file to .env and fill in your actual API keys
# The .env file is excluded from version control for security

# ============================================================================
# ALPACA TRADING (REQUIRED)
# ============================================================================

# Alpaca Markets - Paper Trading
# Get keys at: https://app.alpaca.markets/signup
ALPACA_API_KEY=your_alpaca_key_here
ALPACA_SECRET_KEY=your_alpaca_secret_here
ALPACA_BASE_URL=https://paper-api.alpaca.markets

# ============================================================================
# TELEGRAM NOTIFICATIONS (OPTIONAL BUT RECOMMENDED)
# ============================================================================

# Create a bot with @BotFather on Telegram
# Get chat ID by messaging @userinfobot
TELEGRAM_BOT_TOKEN=your_telegram_bot_token_here
TELEGRAM_CHAT_ID=your_telegram_chat_id_here
TELEGRAM_ENABLED=true

# ============================================================================
# BOT CONFIGURATION
# ============================================================================

# Logging
BOT_LOG_LEVEL=INFO

# Data storage
BOT_DATA_DIR=data
BOT_LOGS_DIR=logs
BOT_TRADES_DB=data/trades.db

# Dry run mode (no real orders)
BOT_DRY_RUN=false

# ============================================================================
# TRADING CONFIGURATION
# ============================================================================

# Symbols to trade (comma-separated)
TRADING_SYMBOLS=AAPL,MSFT,GOOGL

# Timeframe for analysis
TRADING_TIMEFRAME=1D

# Lookback period in days
TRADING_LOOKBACK_DAYS=252

# Rebalance threshold (5% = 0.05)
TRADING_REBALANCE_THRESHOLD=0.05

# ============================================================================
# RISK MANAGEMENT
# ============================================================================

# Max position size as % of portfolio
RISK_MAX_POSITION_PCT=0.25

# Max single order value ($)
RISK_MAX_ORDER_VALUE=50000

# Max daily loss before stopping (%)
RISK_MAX_DAILY_LOSS_PCT=0.05

# Minimum cash reserve (%)
RISK_MIN_RESERVE=0.10

# Max number of concurrent positions
RISK_MAX_POSITIONS=20

# ============================================================================
# DATA PROVIDERS (OPTIONAL)
# ============================================================================

# FRED - Federal Reserve Economic Data (FREE)
# Get key at: https://fred.stlouisfed.org/docs/api/api_key.html
FRED_API_KEY=your_fred_key_here

# Tradier - Options data (requires account)
TRADIER_API_KEY=your_tradier_key_here

# Finnhub - Alternative data, earnings (free tier available)
FINNHUB_API_KEY=your_finnhub_key_here

# Alpha Vantage - Fundamental data (free tier available)
ALPHA_VANTAGE_API_KEY=your_alphavantage_key_here

# ============================================================================
# CLOUD & COMPUTE (OPTIONAL)
# ============================================================================

# RunPod API Key - GPU cloud training
RUNPOD_API_KEY=your_runpod_api_key_here

# ============================================================================
# EXPERIMENT TRACKING (OPTIONAL)
# ============================================================================

# Weights & Biases API Key
WANDB_API_KEY=your_wandb_api_key_here
WANDB_PROJECT=hybrid-trading-model

# ============================================================================
# UNIVERSE SCREENING
# ============================================================================

# Base universe type: sp500, sp100, nasdaq100, dow30, russell1000, russell2000, custom
UNIVERSE_TYPE=sp500

# Custom symbols to add (comma-separated)
UNIVERSE_CUSTOM_SYMBOLS=

# Symbols to exclude (comma-separated)
UNIVERSE_EXCLUDE_SYMBOLS=

# Maximum universe size after screening
MAX_UNIVERSE_SIZE=100

# Price filters
SCREEN_MIN_PRICE=5.0
# SCREEN_MAX_PRICE=          # Uncomment to set max price

# Liquidity filters
SCREEN_MIN_AVG_VOLUME=500000
SCREEN_MIN_DOLLAR_VOLUME=10000000
SCREEN_MIN_MARKET_CAP=10000000000
# SCREEN_MAX_MARKET_CAP=     # Uncomment to set max market cap

# Float and tradability
SCREEN_MIN_FLOAT=50000000
SCREEN_MIN_DAYS_TRADING=252

# Sector exclusions (comma-separated)
UNIVERSE_EXCLUDE_SECTORS=

# Other filters
SCREEN_EXCLUDE_ADRS=false
SCREEN_EXCLUDE_ETFS=true

# ============================================================================
# PRICE DATA
# ============================================================================

# Historical depth
PRICE_LOOKBACK_DAYS=504
PRICE_INTRADAY_DAYS=5

# Data to fetch
PRICE_FETCH_DAILY=true
PRICE_FETCH_INTRADAY=true
PRICE_INTRADAY_FREQ=5m

# Price adjustments
PRICE_USE_ADJUSTED=true

# Derived data
PRICE_COMPUTE_RETURNS=true
PRICE_COMPUTE_LOG_RETURNS=true
PRICE_COMPUTE_CUM_RETURNS=true

# ============================================================================
# FUNDAMENTAL DATA
# ============================================================================

# Master enable
FUNDAMENTAL_ENABLED=true

# Categories to fetch
FUNDAMENTAL_VALUATION=true
FUNDAMENTAL_PROFITABILITY=true
FUNDAMENTAL_GROWTH=true
FUNDAMENTAL_HEALTH=true
FUNDAMENTAL_CASH_FLOW=true
FUNDAMENTAL_DIVIDENDS=true
FUNDAMENTAL_EARNINGS=true

# Historical periods (quarters)
FUNDAMENTAL_PERIODS=12

# ============================================================================
# TECHNICAL INDICATORS
# ============================================================================

# Master enable
TECHNICAL_ENABLED=true

# Indicator categories
TECH_TREND=true
TECH_MACD=true
TECH_ADX=true
TECH_MOMENTUM=true
TECH_VOLATILITY=true
TECH_VOLUME=true
TECH_PIVOTS=true
TECH_FIBONACCI=true

# Moving average periods (comma-separated)
TECH_SMA_PERIODS=10,20,50,100,200
TECH_EMA_PERIODS=9,12,21,26,50

# Indicator periods
TECH_RSI_PERIOD=14
TECH_ATR_PERIOD=14

# ============================================================================
# RISK METRICS
# ============================================================================

# Master enable
RISK_METRICS_ENABLED=true

# Volatility
RISK_VOLATILITY=true
RISK_VOL_WINDOWS=20,60,252

# Beta and correlation
RISK_BETA=true
RISK_BENCHMARK=SPY

# Drawdowns
RISK_DRAWDOWNS=true

# Value at Risk
RISK_VAR=true
RISK_VAR_LEVELS=0.95,0.99

# CVaR (Expected Shortfall)
RISK_CVAR=true

# Risk-adjusted returns
RISK_SHARPE=true
RISK_RF_SOURCE=FRED

# ============================================================================
# MARKET MICROSTRUCTURE
# ============================================================================

# Master enable
MICROSTRUCTURE_ENABLED=true

# Data categories
MICRO_BID_ASK=true
MICRO_SHORT_INTEREST=true
MICRO_INSTITUTIONAL=true
MICRO_SHARE_STATS=true

# ============================================================================
# ALTERNATIVE DATA
# ============================================================================

# Master enable (requires API keys)
ALT_DATA_ENABLED=false

# Data categories
ALT_ANALYST=true
ALT_ESG=false
ALT_NEWS=false
ALT_SOCIAL=false
ALT_OPTIONS=false

# ============================================================================
# CORPORATE EVENTS
# ============================================================================

# Master enable
EVENTS_ENABLED=true

# Event categories
EVENTS_EARNINGS=true
EVENTS_DIVIDENDS=true
EVENTS_SPLITS=true
EVENTS_INSIDER=true
EVENTS_SEC=false

# ============================================================================
# STOCK SELECTION
# ============================================================================

# Selection method: feature_scoring, multi_factor, momentum, value, quality, growth, dividend, low_volatility, combined
SELECTION_METHOD=multi_factor

# Number of stocks to select
NUM_STOCKS_SELECT=20

# Factor weights (for multi_factor method)
FACTOR_MOMENTUM_WEIGHT=0.25
FACTOR_VALUE_WEIGHT=0.25
FACTOR_QUALITY_WEIGHT=0.25
FACTOR_GROWTH_WEIGHT=0.15
FACTOR_LOWVOL_WEIGHT=0.10

# Momentum lookback periods (trading days)
MOMENTUM_SHORT=20
MOMENTUM_MEDIUM=60
MOMENTUM_LONG=252

# Sector constraints
MAX_SECTOR_WEIGHT=0.30
MIN_SECTORS=3

# ============================================================================
# DATA PROVIDERS
# ============================================================================

# Primary provider: yfinance, alpaca, polygon
DATA_PRIMARY_PROVIDER=yfinance

# Fallback providers (comma-separated)
DATA_FALLBACK_PROVIDERS=alpaca,alphavantage

# Live quotes
ENABLE_LIVE_QUOTES=true
LIVE_QUOTE_PROVIDER=alpaca

# Caching
DATA_CACHE_ENABLED=true
DATA_CACHE_HOURS=4
DATA_CACHE_DIR=data/cache

# Rate limiting and timeouts
DATA_RATE_LIMIT=100
DATA_TIMEOUT=30

# ============================================================================
# PROJECT
# ============================================================================

PROJECT_NAME=stock_analysis
