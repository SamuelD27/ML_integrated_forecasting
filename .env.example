# API Keys Example File
# Copy this file to .env and fill in your actual API keys
# The .env file is excluded from version control for security

# ============================================================================
# DATA PROVIDERS (for enhanced market data)
# ============================================================================

# Alpaca Markets - Intraday data, real-time quotes (FREE TIER AVAILABLE)
# Get keys at: https://app.alpaca.markets/signup
ALPACA_API_KEY=your_alpaca_key_here
ALPACA_API_SECRET=your_alpaca_secret_here
ALPACA_BASE_URL=https://paper-api.alpaca.markets

# FRED - Federal Reserve Economic Data (FREE)
# Get key at: https://fred.stlouisfed.org/docs/api/api_key.html
FRED_API_KEY=your_fred_key_here

# Tradier - Options data (requires account)
TRADIER_API_KEY=your_tradier_key_here

# Finnhub - Alternative data, earnings (free tier available)
FINNHUB_API_KEY=your_finnhub_key_here

# Alpha Vantage - Fundamental data (free tier available)
ALPHA_VANTAGE_API_KEY=your_alphavantage_key_here

# ============================================================================
# CLOUD & COMPUTE
# ============================================================================

# RunPod API Key - GPU cloud training
# Get your key from: https://www.runpod.io/console/user/settings
RUNPOD_API_KEY=your_runpod_api_key_here

# ============================================================================
# EXPERIMENT TRACKING
# ============================================================================

# Weights & Biases API Key
# Get your key from: https://wandb.ai/authorize
WANDB_API_KEY=your_wandb_api_key_here

# ============================================================================
# PROJECT CONFIGURATION
# ============================================================================

PROJECT_NAME=stock_analysis
WANDB_PROJECT=hybrid-trading-model
